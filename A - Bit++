import java.util.*;
public class Bit {
 
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int ans=0;
		for(int i=0;i<=n-1;i++) {
			String s=sc.next();
			if(s.equals("X++") || s.equals("++X")) {
				ans++;
			}
			else {
				ans--;
			}
		}
		System.out.println(ans);
 
	}
 
}
